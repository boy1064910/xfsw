<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="KnowledgePointContentAnswer">
	<select id="selectListByKnowledgePointContentIds" parameterType="java.util.List" resultType="KnowledgePointContentAnswer">
		SELECT * FROM KnowledgePointContentAnswer WHERE knowledgePointContentId IN
		<foreach collection="list" item="id" open="(" close=")" separator=",">
			#{id}
		</foreach>
	</select>
</mapper>